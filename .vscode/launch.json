    "version": "0.2.0",
    "configurations": [
        {
        "name": "Launch Debug",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/bin/Debug-windows-x86_64/Osengine.exe",
        "args": [
            "examples/classes.os",
            "--execute",
            "--debug",
        ],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb"
        }
    ]
}
